{
  "name": "express-mongoose-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Microservices based express-mongoose-typescript-boilerplate ",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "tsc": "tsc",
    "watch": "tsnd --respawn src/index.ts user_service",
    "prettify": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "mig": "node dist/index.js migration",
    "lint:fix": "eslint --fix src",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register -r dotenv/config 'src/**/*test.ts'",
    "testx": "env TS_NODE_COMPILER_OPTIONS='{\\\"module\\\": \\\"commonjs\\\" }' mocha -r ts-node/register -r dotenv/config 'src/Microservices/Routes/UserRouter.test.ts'",
    "coverage": "nyc --reporter=text npm run test"
  },
  "author": "Qodehive",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1130.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "cls-rtracer": "^2.6.0",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "9.0.0",
    "moment": "2.29.4",
    "moment-timezone": "0.5.35",
    "mongoose": "6.4.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.1",
    "stack-trace": "0.0.10",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.2.0",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
